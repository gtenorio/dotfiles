{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.ignoreMissingGitWarning": true,
    "python.linting.enabled": false,
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
    "terminal.integrated.rightClickCopyPaste": true,
    "python.pythonPath": "/usr/local/opt/python@2/bin/python2.7",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.explorerKind": "external",
    "workbench.statusBar.feedback.visible": false,
    "editor.tabSize": 2,
    "workbench.colorTheme": "Oceanic Next Bimbo Dark",
    "workbench.colorCustomizations": {
        "sideBar.background": "#050507",
        "sideBar.foreground": "#bfbfbf",
        "sideBarSectionHeader.background": "#17171b",
        "activityBar.background": "#17171b",
        "activityBar.foreground": "#bfbfbf",
        "activityBarBadge.background": "#3a65c9",
        //"activityBar.border": "#2f2f31",
        "editorGroupHeader.tabsBackground": "#050507",
        //"editorGroupHeader.tabsBorder": "#2f2f31",
        //"tab.inactiveBackground": "#171721",
        "tab.inactiveBackground": "#050507",
        "tab.inactiveForeground": "#bfbfbf",
        "tab.activeBorder": "#0e0e12",
        "tab.activeBorderTop": "#2f2f31",
        "tab.unfocusedActiveBorder": "#0e0e12",
        //"tab.border": "#2f2f31",
        "tab.border": "#121219",
        "tab.hoverBackground": "#2b2b35",
        "editor.background": "#0e0e12",
        "peekView.border": "#3a65c9",
        "statusBar.foreground": "#bfbfbf",
        "titleBar.activeBackground": "#121219",
        "titleBar.inactiveBackground": "#121219",
        "titleBar.border": "#050507",
        //"sideBar.border": "#2f2f31",
        "scrollbar.shadow": "#0e0e12",
        "peekViewEditor.background": "#050507",
        "peekViewTitle.background": "#050507",
        "peekViewEditor.matchHighlightBackground": "#2f2f31",
        "peekViewResult.background": "#050507",
        "peekViewResult.matchHighlightBackground": "#2f2f31",
        "peekViewResult.selectionBackground": "#171721",
        "statusBar.background": "#1b1b25",
	"statusBar.noFolderBackground": "#1b1b25",
        "input.background": "#171721",
        "editorWidget.background": "#2a2a3a"
    },
    "window.zoomLevel": 2.3,
    "window.titleBarStyle": "custom"
}
